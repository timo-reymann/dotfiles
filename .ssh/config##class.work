IdentityFile ~/.ssh/id_rsa

# only hosts starting with letters, and reachable over ssh proxy
Match exec "echo %h | grep '^[a-z].*' && nc -z %h.ssh.proxy 22 2>/dev/null"
    Hostname %h.ssh.proxy
    IdentityFile ~/.ssh/id_rsa

# default user for every vm
Host *
    User treymann

# git user for gitlab
Host gitlab.witt-gruppe.eu
    User git

