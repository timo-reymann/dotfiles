{
  "presets": {
    "Middle Tier": {
      "useConfigFiles": false,
      "plugins": {
        "@vue/cli-plugin-babel": {},
        "@vue/cli-plugin-pwa": {},
        "@vue/cli-plugin-eslint": {
          "config": "base",
          "lintOn": [
            "save",
            "commit"
          ]
        }
      },
      "router": true,
      "routerHistoryMode": false,
      "vuex": true
    },
    "minimal-eror-linting": {
      "useConfigFiles": true,
      "plugins": {
        "@vue/cli-plugin-eslint": {
          "config": "base",
          "lintOn": [
            "save"
          ]
        }
      },
      "router": true,
      "routerHistoryMode": true
    }
  },
  "useTaobaoRegistry": false,
  "packageManager": "yarn"
}